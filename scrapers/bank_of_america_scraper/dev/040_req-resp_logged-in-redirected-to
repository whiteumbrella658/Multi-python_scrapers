<script type="text/javascript">
function registerDigitalData() {
		var cmModalPageId = "OLB:Mkt:AcOLB:Mkt:Accounts;SO_AutoMod";
		var pageIdLength =digitalData.page.pageInfo.length;
		digitalData.page.pageInfo.push({pageID : cmModalPageId});
		digitalData.page.category.addlCategory="OLB:Mkt:Accounts";
		$('#modalOffer').click(function(event){
			bactm_captureAddlPageview(pageIdLength,1);
		});
		$('#offers-grid-module-modalClose').on('click', function(){
			bactm_restorePageID(0);
		});
	}
	if( window.boa && window.boa.digitalData ) {
        window.boa.digitalData.register( registerDigitalData );
    }
    else {
        try{
            registerDigitalData();
        } catch(e) {}
    }
</script>
</div>
