<script>
try {
  var pause = 250;

  function cleanURL(url) {
      url = url.replace(/(http:|https:)/g, "");
      return (url.indexOf("//") != 0 ? "//" : "") + url;
  }

  function ajaxURL(url, pause) {
      setTimeout(function() {
          $.ajax({
              url: url,
              dataType: "script",
              cache: true,
              xhrFields: {
                  withCredentials: true
              }
          });
      }, pause);
  }

  function getURL(url, pause) {
      if (typeof url === "string") {
          url = [url];
      }
      pause = typeof pause === "number" ? pause : 0;
      for (var i = 0; i < url.length; i++) {
          ajaxURL(cleanURL(url[i]), pause * i);
      }
  }
  $(function() {
	  setTimeout(function() {
  		getURL([ "//aero.bankofamerica.com/30306/I3n.js", "dull.bankofamerica.com/boaa/y9h.js" ], pause );
  	}, pause );
  });

} catch (e) {}
</script>


